<% include ../../partials/admin-header.ejs %>

<div class="container">
    <div class="row">
<h1 style="text-align: center">Create a new project</h1>

        <div class="form-centering">
        <form action="/" method="POST">
            <div class="form-group">
            <input class="form-control" type="text" name="topTitle" placeholder="name">
            </div>
            <div class="form-group">
            <input class="form-control"  type="text" name="topSmallTitle" placeholder="image">
            </div>
             <div class="form-group">
            <input class="form-control" type="text" name="topDescription" placeholder="stack">
            </div>

            <div class="form-group">
            <input class="form-control" type="text" name="bottomTitle" placeholder="pdescription">
            </div>   
            <div class="form-group">
            <input class="form-control" type="text" name="bottomSmallTitle" placeholder="pimage">
            </div> 
            <div class="form-group">
            <input class="form-control" type="text" name="bottomDescription" placeholder="link">
            </div> 

            <div class="form-group">
            <button class="btn btn-lg btn-default btn-block btn-primary">Submit!</button>
            </div>           
            
    
    </form>
    <a href="/">Back to the Homepage</a> 

